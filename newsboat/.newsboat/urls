# Query Aggregators
# https://newsboat.org/releases/2.10.2/docs/newsboat.html#available-comparison-operators
"query:Tech News:tags # \"tech\" and tags !# \"vendor\""
"query:Nix News:tags # \"nix\""
"query:FreeBSD News:tags # \"freebsd\""
"query:Comics:tags # \"comics\""
"query:World News:tags # \"news\""
"query:HAM:tags # \"ham\""
"query:Tech Vendors:tags # \"vendor\" and tags # \"tech\""
"query:Status:tags # \"status\""
